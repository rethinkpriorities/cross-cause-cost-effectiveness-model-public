  "scripts": {
    "dev": "VITE_APP_VERSION=$(node -p -e \"require('./package.json').version\") vite",
    "build": "tsc && VITE_APP_VERSION=$(node -p -e \"require('./package.json').version\") vite build",
    "lint": "eslint ./src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \".\"",
    "format-check": "prettier --check \".\"",
    "test": "npx jest",
    "preview": "vite preview",
    "generate-client": "openapi --input http://127.0.0.1:8000/openapi.json --output ./src/client --useUnionTypes --indent 2",
    "type-check": "tsc --noEmit --incremental false --pretty",
    "cypress:open": "cypress open",
    "test-cypress": "sh ./scripts/test_cypress.sh"
  },
